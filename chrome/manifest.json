{
  "description": "Make instagram less addictive",
  "icons": {
    "16": "b16.png",
    "48": "b48.png",
    "128": "b128.png"
  },
  "name": "Boringstagram",
  "permissions": [
    "https://www.instagram.com/*"
  ],
  "version": "0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://www.instagram.com/*"
      ],
      "css": [
        "boringstagram.css"
      ]
    }
  ]
}
