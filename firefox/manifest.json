{
  "description": "Make instagram less addictive",
  "icons": {
    "16": "b.16.png",
    "128": "b.128.png"
  },
  "name": "Boringstagram",
  "permissions": [
    "https://www.instagram.com/*"
  ],
  "version": "0.1.2",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://www.instagram.com/*"
      ],
      "css": [
        "boringstagram.css"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {}
  }
}
